_homeshick()
{
  cur=${COMP_WORDS[COMP_CWORD]}
  case "${cur}" in
    c*)   use="cd" ;;
    cl*)  use="clone" ;;
    ch*)  use="check" ;;
    g*)   use="generate" ;;
    lis*) use="list" ;;
    lin*) use="link" ;;
    p*)   use="pull" ;;
    r*)   use="refresh" ;;
    t*)   use="track" ;;
    h*)   use="help" ;;
  esac
  COMPREPLY=( $( compgen -W "$use" -- $cur ) )
}
complete -o default -o nospace -F _homeshick homeshick
    
